<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:macro name="gz_joint" params="name mu1 mu2 kp kd maxVel dampingFactor">

        <gazebo reference="${name}_link">
            <mu1 value="${mu1}"/>
            <mu2 value="${mu2}"/>
            <kp value="${kp_}" />
            <kd value="${kd}" />
            <maxVel value="${maxVel}"/>
            <dampingFactor value="${dampingFactor}"/>
        </gazebo>

    </xacro:macro>

</robot>