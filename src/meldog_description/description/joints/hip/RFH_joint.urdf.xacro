<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <joint name="RFH_joint" type="continuous">
        <parent link="RFH_link" /> 
        <child link="RFUL_link" />
        <origin xyz="${H_x}
                     ${-H_y}
                     ${H_z}" 
                rpy="0 0 0" />
        <limit effort="${H_effort}" 
               velocity="${H_velocity}" />
        <axis xyz="0 -1 0" />
    </joint>

</robot>